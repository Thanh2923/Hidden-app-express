<%- include("../layouts/head")%>
<div id="main">
    <div class="product-spbc my-5">
    <div class="container">
       <div class="title-product d-flex">
        <span class="fw-bold fs-5" style="width: 20%;" >TẤT CẢ SẢN PHẨM</span>
        <p style="width: 80%; border: 1px solid #ebebeb;; height: 1px;"  class="m-3" ></p>
       </div>

    </div>
     <div class="spbc">
      <div class="container-fluid ">
        <div class="row my-5">
            <% posts.forEach((pro)=>{ %>
                <div class="col-lg-3 col-md-3 col-sm-6">
                    <a class="text-decoration-none text-black" href="/chitietsanpham/<%=pro.id%>">
                    <img  class="img-product" src="../images/<%=pro.image%>" style="width: 175px; height: 176px;" alt="">
                    <h6 class="mt-3"><%=pro.name%></h6>
                    <div class="price d-flex  m-2">
                         <span class="text-danger fw-bolder  "><%=pro.price%></span> <p class=" ps-3 text-decoration-line-through " style="color: #acb2b0;" >450.000đ</p>
                    </div>
                </a>
                  </div>
                <%})%>
        

         
        </div>
      </div>
     </div>
  </div>
  

  
</div> 

<%- include("../layouts/footer")%>
